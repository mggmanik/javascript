(function(){
	
	var arr1 = [2,3,7,8,10];
  var arr2 = [7,5,4,10,11];
  
  var arr3 = arr1.concat(arr2);
  
	function removeDuplicates(arr){
    var removed = arr.filter(function(elem, index, self) {
        return index == self.indexOf(elem);
    });
    return removed;
}

console.log(removeDuplicates(arr3));

}  
());
